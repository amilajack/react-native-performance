    "react-native",
    "performance",
    "perf",
    "benchmark"
  ],
  "author": "Joel Arvidsson",
  "license": "MIT",
  "peerDependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.3"
  },
  "codegenConfig": {
    "name": "RNPerformanceSpec",
    "type": "modules",
    "jsSrcsDir": "./src",
    "android": {
      "javaPackageName": "com.oblador.performance"
    }
  }
